bool isPalindrome(LinkedListNode<int> *head) {
    
    vector<int> arr;
    LinkedListNode<int> *temp=head;
    //copy all elements in array
    while(temp!=NULL){
        arr.push_back(temp->data);
        temp=temp->next;
    }

    int s=0;
    int e=arr.size()-1;
    //2 pointer approach
    while(s<=e){
        if(arr[s]!=arr[e]){
            return false;
        }
        else{
            s++;
            e--;
        }
    }
 return true;
}
